{
  "version": 2,
  "framework": "vite",
  "buildCommand": "cd react-frontend && npm install && npm run build",
  "outputDirectory": "react-frontend/dist",
  "builds": [
    {
      "src": "react-frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "react-frontend/dist/$1"
    }
  ]
}
